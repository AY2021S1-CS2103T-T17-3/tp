@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant "n: PersonNameContainsKeywordsPredicate" as PersonNameContainsKeywordsPredicate LOGIC_COLOR
participant "e: PersonExperienceContainsKeywordsPredicate" as PersonExperienceContainsKeywordsPredicate LOGIC_COLOR
participant "f : FindPersonCommand" as FindPersonCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR

end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("find can n/Alex exp/0")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("find can n/Alex exp/0")
activate AddressBookParser

create FindCommandParser
AddressBookParser -> FindCommandParser
activate FindCommandParser

FindCommandParser --> AddressBookParser
deactivate FindCommandParser

AddressBookParser -> FindCommandParser : parse("find can n/Alex exp/0")
activate FindCommandParser

create PersonNameContainsKeywordsPredicate
FindCommandParser -> PersonNameContainsKeywordsPredicate : PersonNameContainsKeywordsPredicate()
activate PersonNameContainsKeywordsPredicate

create PersonExperienceContainsKeywordsPredicate
FindCommandParser -> PersonExperienceContainsKeywordsPredicate : PersonExperienceContainsKeywordsPredicate()
activate PersonExperienceContainsKeywordsPredicate

create FindPersonCommand
FindCommandParser -> FindPersonCommand : FindPersonCommand()
activate FindPersonCommand

FindPersonCommand --> FindCommandParser : f
deactivate FindPersonCommand

FindCommandParser --> AddressBookParser : f
deactivate FindCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
FindCommandParser -[hidden]-> AddressBookParser
destroy FindCommandParser

AddressBookParser --> LogicManager : f
deactivate AddressBookParser

LogicManager -> FindPersonCommand : execute()
activate FindPersonCommand

FindPersonCommand -> FindPersonCommand : composePredicatesList({n, e})
activate FindPersonCommand
FindPersonCommand --> FindPersonCommand : n && e
deactivate FindPersonCommand

FindPersonCommand -> Model : updateFilteredPersonList(n && e)
activate Model

Model --> FindPersonCommand
deactivate Model

destroy PersonNameContainsKeywordsPredicate
destroy PersonExperienceContainsKeywordsPredicate

create CommandResult
FindPersonCommand -> CommandResult
activate CommandResult

CommandResult --> FindPersonCommand
deactivate CommandResult

FindPersonCommand --> LogicManager : "10 persons listed!"
deactivate FindPersonCommand

[<--LogicManager
deactivate LogicManager
@enduml
